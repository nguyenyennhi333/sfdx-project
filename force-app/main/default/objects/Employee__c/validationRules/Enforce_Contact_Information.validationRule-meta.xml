<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enforce_Contact_Information</fullName>
    <active>true</active>
    <errorConditionFormula>(ISPICKVAL( How_Could_We_Contact__c ,&quot;Phone&quot;) &amp;&amp; ISBLANK( Phone__c)) || 
(ISPICKVAL( How_Could_We_Contact__c ,&quot;Email&quot;) &amp;&amp; ISBLANK(Email__c ))</errorConditionFormula>
    <errorMessage>Please enter Phone OR Email.</errorMessage>
</ValidationRule>
